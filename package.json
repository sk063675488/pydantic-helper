{
  "name": "pydantic-helper",
  "displayName": "Pydantic Helper & Snippets",
  "description": "Modern Pydantic v2 snippets and helpers for Python developers",
  "version": "1.0.1",
  "publisher": "darsh-technology06",
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1f2937",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sk063675488/pydantic-helper.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/pydantic.code-snippets.json"
      }
    ],
    "commands": [
      {
        "command": "pydantic-helper.insertModel",
        "title": "Pydantic: Insert BaseModel"
      },
      {
        "command": "pydantic-helper.insertModel",
        "title": "Pydantic: Insert BaseModel"
      },
      {
        "command": "pydantic-helper.insertFastAPIApp",
        "title": "FastAPI: Insert App"
      },
      {
        "command": "pydantic-helper.insertFastAPIModels",
        "title": "FastAPI: Insert Request & Response Models"
      },
      {
        "command": "pydantic-helper.insertFastAPICRUD",
        "title": "FastAPI: Insert CRUD Endpoint"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.1",
    "path": "^0.12.7"
  }
}